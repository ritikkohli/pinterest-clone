<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="https://cdn.tailwindcss.com"></script>
</head>
<body>
    <h1 class="text-center text-xl font-semibold my-5">All Pins</h1>
    <div class="mx-20 box-border bg-red-400 flex flex-wrap gap-5">
        <% user.posts.forEach(function(pin){ %>
            <div class="w-40 overflow-hidden h-max pin cursor-pointer">
                <img src="/images/uploads/<%=pin.image%>" class="rounded-2xl">
                <p class="m-2 mx-3"><%=pin.postText%></p>
            </div>
        <% }) %>
    </div>
</body>
</html>